{
  "type": "object",
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "maxLength": 32
    },
    "image": {
      "type": "string"
    },
    "properties": {
      "type": "object",
      "properties": {
        "category": {
          "type": "string"
        },
        "creators": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "address": {
                "type": "string"
              }
            },
            "required": [
              "address"
            ]
          }
        }
      },
      "required": [
        "category",
        "creators"
      ]
    },
    "extensions": {
      "type": "object",
      "properties": {
        "solana_dapp_store": {
          "type": "object",
          "properties": {
            "publisher_details": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "website": {
                  "type": "string",
                  "pattern": "^https://"
                },
                "contact": {
                  "type": "string",
                  "pattern": "^[a-zA-Z0-9_!#$%&'*+/=?`{|}~^.-]+@[a-zA-Z0-9.-]+\\.[a-zA-Z]{2,}$"
                }
              },
              "required": [
                "name",
                "website",
                "contact"
              ]
            }
          },
          "required": [
            "publisher_details"
          ]
        }
      },
      "required": [
        "solana_dapp_store"
      ]
    }
  },
  "required": [
    "schema_version",
    "name",
    "image",
    "properties",
    "extensions"
  ]
}
{
  "type": "object",
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "properties": {
      "type": "object",
      "properties": {
        "category": {
          "type": "string"
        },
        "creators": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "address": {
                "type": "string"
              }
            },
            "required": ["address"]
          }
        }
      }
    },
    "extensions": {
      "type": "object",
      "properties": {
        "solana_dapp_store": {
          "type": "object",
          "properties": {
            "publisher_details": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "website": {
                  "type": "string"
                },
                "contact": {
                  "type": "string"
                }
              },
              "required": ["name", "website", "contact"]
            },
            "release_details": {
              "type": "object",
              "properties": {
                "version": {
                  "type": "string"
                },
                "updated_on": {
                  "type": "string"
                },
                "license_url": {
                  "type": "string"
                },
                "copyright_url": {
                  "type": "string"
                },
                "privacy_policy_url": {
                  "type": "string"
                },
                "localized_resources": {
                  "type": "object",
                  "properties": {
                    "long_description": {
                      "type": "string"
                    },
                    "new_in_version": {
                      "type": "string"
                    },
                    "saga_features_localized": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    }
                  },
                  "required": ["long_description", "new_in_version", "name"]
                }
              },
              "required": [
                "version",
                "updated_on",
                "license_url",
                "copyright_url",
                "privacy_policy_url",
                "localized_resources"
              ]
            },
            "media": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "mime": {
                    "type": "string"
                  },
                  "purpose": {
                    "type": "string"
                  },
                  "width": {
                    "type": "integer"
                  },
                  "height": {
                    "type": "integer"
                  },
                  "sha256": {
                    "type": "string"
                  }
                },
                "required": ["mime", "purpose", "width", "height", "sha256"]
              }
            },
            "files": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "mime": {
                    "type": "string"
                  },
                  "purpose": {
                    "type": "string"
                  },
                  "size": {
                    "type": "integer"
                  },
                  "sha256": {
                    "type": "string"
                  }
                },
                "required": ["mime", "purpose", "size", "sha256"]
              }
            },
            "android_details": {
              "type": "object",
              "properties": {
                "android_package": {
                  "type": "string"
                },
                "version_code": {
                  "type": "integer"
                },
                "min_sdk": {
                  "type": "integer"
                },
                "permissions": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "locales": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              },
              "required": [
                "android_package",
                "version_code",
                "min_sdk",
                "permissions",
                "locales"
              ]
            }
          },
          "required": [
            "publisher_details",
            "release_details",
            "media",
            "files",
            "android_details"
          ]
        },
        "i18n": {
          "type": "object",
          "patternProperties": {
            "([A-Za-z0-9\\-]+)\\w+/gi": {
              "type": "object",
              "patternProperties": {
                "([A-Za-z0-9\\-]+)\\w+/gi": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      "required": ["solana_dapp_store", "i18n"]
    }
  },
  "required": [
    "schema_version",
    "name",
    "description",
    "image",
    "properties",
    "extensions"
  ]
}

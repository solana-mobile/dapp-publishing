{
  "name": "@solana-mobile/dapp-store-publishing-tools",
  "version": "0.7.3",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.js",
    "types": "./lib/types/index.d.ts"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx mkdir -p lib && shx rm -rf lib",
    "prebuild": "pnpm run clean && pnpm run generate-types",
    "build": "swc src -d lib --copy-files",
    "watch": "swc src --out-dir lib -w",
    "generate-types": "json2ts -i src/schemas -o src/generated"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/mime": "^3.0.1",
    "@types/node-fetch": "^2.6.2",
    "json-schema-to-typescript": "^11.0.2",
    "shx": "^0.3.4"
  },
  "dependencies": {
    "@metaplex-foundation/js": "0.18.3",
    "@solana/web3.js": "1.68.0",
    "ajv": "^8.11.0",
    "axios": "1.1.3",
    "chokidar": "^3.5.3",
    "debug": "^4.3.4",
    "image-size": "^1.0.2",
    "mime": "^3.0.0"
  }
}
